{
    "name": "sp4ceb4r/gnip-data-generator",
    "description": "Simple library to generate faked Gnip data.",
    "type": "library",
    "version": "0.2.0",
    "keywords": [ "gnip", "faker" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jacob Schofield"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "fzaninotto/faker": "^1.7",
        "nesbot/carbon": "^1.23"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "illuminate/support": "^5.4.0"
    },
    "autoload": {
        "psr-4": {
            "Sp4ceb4r\\GnipDataGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sp4ceb4r\\GnipDataGenerator\\Testing\\": "tests/"
        }
    }
}
